.skill-component {
  font-weight: normal;
  font-size: 16px;
  color: #414C63;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.title {
  font-size: 16px;
  width: 57%;
}

.percent {
  font-size: 16px;
  width: 19%;
}

.buttons {
  width: 19%;
  display: flex;
  justify-content: flex-end;
}

.btn {
  margin-right: 20px;

  &:last-child {
    margin-right: 0;
  }
}