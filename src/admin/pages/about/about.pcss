.page-content {
  padding: 60px 0 20px;
  background: url("../../../images/content/admin-bg.png") center center / cover no-repeat;
  flex: 1;
}

.header {
  display: flex;
  margin-bottom: 50px;
}

.title {
  font-weight: bold;
  font-size: 21px;
  color: $text-color;
  margin-right: 60px;
}

.skills {
  display: flex;
  flex-wrap: wrap;
}

.item {
  width: calc(100% / 2 - 30px);
  margin-right: 30px;
}